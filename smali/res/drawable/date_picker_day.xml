<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:drawable="@drawable/datepicker_range_first" app:tsquare_state_range_first="true" />
    <item android:drawable="@drawable/datepicker_range_last" app:tsquare_state_range_last="true" />
    <item android:drawable="@drawable/datepicker_range_middle" app:tsquare_state_range_middle="true" />
    <item android:state_selected="true" android:drawable="@drawable/datepicker_circle" />
    <item android:state_pressed="true" android:drawable="@drawable/datepicker_circle" />
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent" />
        </shape>
    </item>
</selector>