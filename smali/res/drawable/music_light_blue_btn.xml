<?xml version="1.0" encoding="utf-8"?>
<selector android:color="@color/music_check_button_bg_pressed"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/music_follow_button_bg_normal" />
            <corners android:radius="@dimen/music_light_blue_button_corners" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/music_light_blue_button_corners" />
            <solid android:color="@color/music_check_button_bg_checked" />
        </shape>
    </item>
</selector>