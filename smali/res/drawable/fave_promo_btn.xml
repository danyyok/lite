<?xml version="1.0" encoding="utf-8"?>
<selector android:color="@color/music_check_button_bg_pressed"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/fave_promo_btn_pressed" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="8.0dip" />
            <solid android:color="@color/fave_promo_btn_normal" />
        </shape>
    </item>
</selector>